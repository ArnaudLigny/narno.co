<!DOCTYPE html>
<html lang="en">
{% include 'include/head.html.twig' %}
  <body>
    <div class="container content">
      <header class="masthead">
        <h3 class="masthead-title">
          <a href="{{ site.baseurl }}" title="Home">{{ site.title }}</a>
          <small>{{ site.baseline }}</small>
        </h3>
      </header>
      <main>
        <ul>
{% for page in site.pages|filterBy('section', 'r')|sortByDate if page.redirect is defined %}
          <li><a href="{{ url(page) }}">{{ page.title }}</a> <small>({{ page.date|date("Y/m/d") }})</small><br/>
            <input id="url" type"url" value="{{ url(page, {'canonical': true}) }}" size="75" readonly/><br/>
            <input id="url" type"url" value="{{ url(page.destination, {'canonical': true}) }}" size="75" readonly/>
          </li>
{% endfor %}
        </ul>
      </main>
    </div>
  </body>
</html>
